{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/.venv": true
    },
    "python.formatting.provider": "black",
    "cSpell.words": [
        "bwm",
        "DBAPI",
        "envvar",
        "paramstyle",
        "pyfile"
    ],
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "debug.toolBarLocation": "docked",
    "terminal.integrated.copyOnSelection": true
}